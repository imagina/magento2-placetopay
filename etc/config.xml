<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <payment>
            <placetopay>
                <active>1</active>
                <title>Placetp Latam</title>

                <order_status_new>pending</order_status_new>
                <order_status_holded>holded</order_status_holded>
                <order_status_processing>processing</order_status_processing>

                <model>Imagina\Placetopay\Model\Placetopay</model>
                <allowspecific>0</allowspecific>

            </placetopay>
        </payment>
    </default>
</config>
